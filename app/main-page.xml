<Page xmlns="http://www.nativescript.org/tns.xsd" loaded="pageLoaded">
  <StackLayout>
    <Label text="Tap the button" cssClass="title" loaded="labelLoaded" />
    <Button text="TAP" tap="{{ tapAction }}" loaded="buttonLoaded" />
    <Label text="{{ message }}" cssClass="message" textWrap="true"/>
  </StackLayout>
</Page>